<%= form_with(model: @user, url: users_path ) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <label> Password </label>
  <%= f.password_field :password_digest %>
  <label> Confirmation de password </label>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "S'inscrire" %>
<% end %>
